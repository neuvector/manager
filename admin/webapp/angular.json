{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "neuvector": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "websrc",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "root",
            "index": "websrc/index.html",
            "main": "websrc/main.ts",
            "polyfills": "websrc/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "allowedCommonJsDependencies": [
              "lodash",
              "chart.js",
              "regl",
              "@babel/runtime/regenerator",
              "@antv/matrix-util",
              "ag-grid-community",
              "jsoneditor",
              "jquery",
              "node-forge",
              "file-saver",
              "moment",
              "screenfull",
              "@antv/algorithm/lib/asyncIndex",
              "color",
              "insert-css"
            ],
            "assets": ["websrc/favicon.ico", "websrc/assets"],
            "styles": [
              "websrc/app/core/systemPrepare/systemPrepare.scss",
              "websrc/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["websrc/app/common/styles"]
            },
            "scripts": [
              "websrc/app/core/jquery/jquerySupport.js",
              "websrc/app/core/systemPrepare/systemPrepare.js"
            ],
            "webWorkerTsConfig": "tsconfig.worker.json",
            "customWebpackConfig": {
              "path": "websrc/nv-webpack.config.js",
              "replaceDuplicatePlugins": true
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "14mb",
                  "maximumError": "16mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "15kb",
                  "maximumError": "30kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "websrc/environments/environment.ts",
                  "with": "websrc/environments/environment.prod.ts"
                }
              ],
              "vendorChunk": true,
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "outputHashing": "all"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "neuvector:build:production"
            },
            "development": {
              "browserTarget": "neuvector:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "neuvector:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "websrc/test.ts",
            "polyfills": "websrc/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": ["websrc/favicon.ico", "websrc/assets"],
            "styles": [
              "websrc/app/core/systemPrepare/systemPrepare.scss",
              "websrc/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["websrc/app/common/styles"]
            },
            "scripts": [
              "websrc/app/core/jquery/jquerySupport.js",
              "websrc/app/core/systemPrepare/systemPrepare.js"
            ],
            "webWorkerTsConfig": "tsconfig.worker.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["websrc/app/**/*.ts", "websrc/app/**/*.html"]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "defaultCollection": "@angular-eslint/schematics",
    "cache": {
      "path": "../.angular/cache"
    }
  }
}
